{
  "name": "ipunkt/laravel-jaeger",
  "description": "Send basic",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Sven Speckmaier",
      "email": "sps@ipunkt.biz"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ipunkt\\LaravelJaeger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ipunkt\\LaravelJaegerTests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.1.3",
    "ramsey/uuid": "^4.1.1",
    "laravel/framework": "^5|^6|^7|^8",
    "code-tool/jaeger-client-php": "^3.2"
  },
  "extra": {
    "laravel": {
        "providers": [
            "Ipunkt\\LaravelJaeger\\Provider"
        ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.1",
    "orchestra/testbench": "^3.8"
  }
}
